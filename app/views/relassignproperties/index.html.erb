<h1>Relassignproperties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relassignproperties.each do |relassignproperty| %>
      <tr>
        <td><%= link_to 'Show', relassignproperty %></td>
        <td><%= link_to 'Edit', edit_relassignproperty_path(relassignproperty) %></td>
        <td><%= link_to 'Destroy', relassignproperty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

