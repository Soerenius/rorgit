<p id="notice"><%= notice %></p>

<h1>Typevalues</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typevalues.each do |typevalue| %>
      <tr>
        <td><%= link_to 'Show', typevalue %></td>
        <td><%= link_to 'Edit', edit_typevalue_path(typevalue) %></td>
        <td><%= link_to 'Destroy', typevalue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typevalue', new_typevalue_path %>
