<p id="notice"><%= notice %></p>

<h1>Externaldocuments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @externaldocuments.each do |externaldocument| %>
      <tr>
        <td><%= link_to 'Show', externaldocument %></td>
        <td><%= link_to 'Edit', edit_externaldocument_path(externaldocument) %></td>
        <td><%= link_to 'Destroy', externaldocument, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Externaldocument', new_externaldocument_path %>
