<p id="notice"><%= notice %></p>

<h1>Relsequences</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relsequences.each do |relsequence| %>
      <tr>
        <td><%= link_to 'Show', relsequence %></td>
        <td><%= link_to 'Edit', edit_relsequence_path(relsequence) %></td>
        <td><%= link_to 'Destroy', relsequence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relsequence', new_relsequence_path %>
